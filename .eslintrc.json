{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "ignorePatterns": ["apiServiceAiraFaceLite.js"],
    "rules": {
        "no-underscore-dangle": 0,
        "import/no-dynamic-require": 0,
        "global-require": 0,
        "no-param-reassign": 0,
        "no-use-before-define": ["error", { "functions": false }]
    }
}